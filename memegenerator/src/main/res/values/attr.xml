<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="VTab">
        <attr name="selected_tint" format="color"/>
        <attr name="selected_background_tint" format="color"/>
        <attr name="item_color" format="color"/>
        <attr name="item_background" format="color"/>
        <attr name="item_padding" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="ToggleImageButton">
        <attr name="checked" format="boolean"/>
        <attr name="checked_background" format="color"/>
        <attr name="unchecked_background" format="color"/>
    </declare-styleable>
    <declare-styleable name="ColorView">
        <attr name="preview_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="CircularProgressButton">
        <attr name="spinning_bar_width" format="dimension"/>
        <attr name="spinning_bar_color" format="color"/>
        <attr name="spinning_bar_padding" format="dimension"/>
        <attr name="initialCornerAngle" format="dimension"/>
        <attr name="finalCornerAngle" format="dimension"/>

    </declare-styleable>

    <declare-styleable name="ColorSeekBar">
        <attr name="colorSeeds" format="reference" />
        <attr name="cornerRadius" format="dimension"/>
        <attr name="barHeight" format="dimension"/>
        <attr name="thumbBorder" format="dimension"/>
        <attr name="thumbBorderColor" format="color"/>
    </declare-styleable>
</resources>